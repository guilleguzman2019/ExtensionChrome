{
    "manifest_version": 2,
    "name": "postulacion_itTalent",
    "version": "0.1.0",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
  
    "permissions": ["<all_urls>","cookies","tabs"],
  
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' https://*.google.com 'unsafe-eval'; object-src 'self'"
  }